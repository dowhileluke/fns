{
  "name": "@dowhileluke/fns",
  "version": "0.0.1",
  "description": "functions that i don't want to copypaste everywhere",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "test": "jest",
    "lint": "xo",
    "prebuild": "rimraf dist/*",
    "build": "rollup -c rollup.config.ts --configPlugin typescript2"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "xo": {
    "semicolon": false,
    "rules": {
      "@typescript-eslint/object-curly-spacing": ["error", "always"],
      "import/extensions": ["error", { "ts": "never" }],
      "unicorn/filename-case": "off"
    }
  },
  "release": {
    "branches": ["main"]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dowhileluke/fns.git"
  },
  "keywords": [
    "utility",
    "util"
  ],
  "author": "dowhileluke",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dowhileluke/fns/issues"
  },
  "homepage": "https://github.com/dowhileluke/fns#readme",
  "devDependencies": {
    "@types/jest": "^28.1.1",
    "jest": "^28.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.75.5",
    "rollup-plugin-typescript2": "^0.32.1",
    "ts-jest": "^28.0.4",
    "tslib": "^2.4.0",
    "typescript": "^4.7.3",
    "xo": "^0.49.0"
  }
}
